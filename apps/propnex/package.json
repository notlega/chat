{
  "name": "@propnex/propnex",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "build:prod": "pnpm run share-static-hardlink && next build",
    "clean": "rimraf ./.next ./out ./coverage ./tsconfig.tsbuildinfo ./node_modules/.cache",
    "dev": "next dev",
    "lint": "next lint",
    "lint:type": "tsc --noEmit --incremental false",
    "share-static-hardlink": "rimraf ./public/shared && syncdir ../../static/assets ./public/shared --copy",
    "share-static-symlink": "rimraf ./public/shared && symlink-dir ../../static/assets ./public/shared",
    "serve": "next start"
  },
  "dependencies": {
    "@cloudinary/react": "^1.11.2",
    "@cloudinary/url-gen": "^1.9.2",
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.6",
    "@mui/material": "^5.11.15",
    "@mui/system": "^5.12.1",
    "@prisma/nextjs-monorepo-workaround-plugin": "^4.13.0",
    "@propnex/db": "workspace:*",
    "bcrypt": "^5.1.0",
    "formik": "^2.2.9",
    "luxon": "^3.3.0",
    "next": "13.1.1",
    "next-auth": "^4.21.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "slate": "^0.91.4",
    "slate-react": "^0.92.0",
    "tslib": "^2.3.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@propnex/configs": "workspace:*",
    "@types/bcryptjs": "^2.4.2",
    "@types/luxon": "^3.3.0",
    "@types/node": "18.15.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "eslint": "8.36.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-react": "7.32.2",
    "rimraf": "^5.0.0",
    "symlink-dir": "^5.1.1",
    "sync-directory": "^6.0.4",
    "typescript": "5.0.2"
  }
}
