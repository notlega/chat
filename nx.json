{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["lint", "build", "test"],
        "cacheDirectory": "tmp/nx/cache"
      }
    },
    "cloud": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint", "test"],
        "accessToken": "OTIxYjVjODYtOTEwNy00NjFmLTk2MGItZmFkNzE1MWIwNTMxfHJlYWQtd3JpdGU="
      }
    }
  },
  "namedInputs": {
    "default": ["{projectRoot}/**/*"]
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"]
    },
    "build:prod": {
      "dependsOn": ["^build"],
      "inputs": ["default", "^default"]
    },
    "dev" : {
      "dependsOn": ["^build"]
    }
  }
