{
  "name": "propnex",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target=build:prod",
    "build:propnex": "nx build:prod @propnex/propnex",
    "build:sandbox": "nx build:prod @propnex/sandbox",
    "dev": "nx run-many --target=serve",
    "dev:propnex": "nx dev @propnex/propnex",
    "dev:sandbox": "nx dev @propnex/sandbox",
    "lint": "nx run-many --target=lint",
    "lint:propnex": "nx lint @propnex/propnex:lint",
    "lint:sandbox": "nx lint @propnex/sandbox:lint",
    "postinstall": "nx run @propnex/db:prisma:generate && echo Please restart your VSCode to make your Prisma Client work.",
    "start": "nx run-many --target=serve",
    "start:propnex": "nx serve @propnex/propnex",
    "start:sandbox": "nx serve @propnex/sandbox",
    "test:propnex": "nx test @propnex/propnex",
    "test:sandbox": "nx test @propnex/sandbox"
  },
  "devDependencies": {
    "@nrwl/nx-cloud": "latest",
    "eslint": "~8.15.0",
    "http-server": "^14.1.1",
    "nx": "15.8.9",
    "prettier": "^2.6.2",
    "prisma": "^4.5.0",
    "ts-node": "10.9.1",
    "tslib": "^2.5.0",
    "typescript": "~4.9.5"
  }
}
